<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tw.gov.idb.base.dao.CityareaDao">
    <select id="selectCityArea" resultType="Cityarea">
        select *
        from cityarea
        order by cityId, areaId
    </select>
</mapper>